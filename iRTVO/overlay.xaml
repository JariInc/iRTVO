<Window x:Class="iRTVO.Overlay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="iRTVO - overlay" Width="1280" Height="720" AllowsTransparency="True" WindowStyle="None" Topmost="True" ResizeMode="CanMinimize" Loaded="Window_Loaded" Name="overlay" Background="#00000000" SizeChanged="Size_Changed" ShowInTaskbar="False">
    <Grid>
        <Canvas Name="vbcanvas">
            <Viewbox Name="viewbox" Stretch="UniformToFill">
                <Canvas Name="canvas" Width="1280" Height="720"/>
            </Viewbox>
        </Canvas>
    </Grid>
</Window>
